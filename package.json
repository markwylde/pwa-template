{
  "name": "pwa-template",
  "version": "1.0.0",
  "description": "A basic pwa template using mithril",
  "main": "index.js",
  "scripts": {
    "build": "node tools/build",
    "analyse": "./node_modules/.bin/esbuild-visualizer --metadata ./meta.json --exclude *.png",
    "watch": "node tools/build --watch",
    "serve": "servatron --spa -d public",
    "start": "npm run watch & npm run serve",
    "lint": "eslint .",
    "test": "node test"
  },
  "keywords": [],
  "author": {
    "name": "Mark Wylde",
    "email": "me@markwylde.com",
    "url": "https://markwylde.com"
  },
  "license": "MIT",
  "dependencies": {
    "chance": "^1.1.8",
    "date-fns": "^2.29.2",
    "esbuild": "^0.15.7",
    "esbuild-visualizer": "^0.3.1",
    "events": "^3.3.0",
    "glob": "^8.0.3",
    "hazkeyboard": "^2.0.0",
    "idb-keyval": "^6.2.0",
    "inobounce": "^0.2.1",
    "location-change-event": "^1.0.2",
    "mithril": "^2.2.2",
    "routemeup": "^2.0.2",
    "spath": "^2.0.4",
    "wtfnode": "^0.9.1"
  },
  "devDependencies": {
    "automage": "^2.10.0",
    "basictap": "^3.4.3",
    "fake-indexeddb": "^4.0.0",
    "jsdom": "^20.0.0",
    "nanoid": "^4.0.0",
    "semistandard": "^16.0.1",
    "servatron": "^2.3.1"
  }
